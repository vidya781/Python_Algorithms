# This is a variant of insertion sort


def Insertionsort(A):
    for i in range(1,len(A)):
        temp = A[i]       # Store the second element of the list in the temp variable
        k = i             # let the values of i be stored in k
        while k > 0 and temp < A[k-1]:         # the while condition is that k which starts at 1 is > 0 and temp 
            A[k] =  A[k-1]                           # should be less than A[0] 
            k -= 1                                   # k's value mow becomes 0
        A[k] = temp                                   # A[0] now becomes 26
    A = [54,26,93,17,31,44,55,20]
    InsertionSort(A)
    print(A)
